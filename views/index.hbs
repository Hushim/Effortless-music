      <input class="file-input" id="uploadFileField" type="file" name="uploadFileField">
      <span class="file-cta">
        <span class="file-icon">
          <i class="fa fa-upload"></i>
        </span>
        <span class="file-label">
          Choose a fileâ€¦
        </span>
      </span>
      <span class="file-name">
        ...
      </span>
    </label>
  </div>

  <div class="column">
    <input class="is-primary" type="button" value="Upload" id="uploadFileButton" />
  </div>
</form>

